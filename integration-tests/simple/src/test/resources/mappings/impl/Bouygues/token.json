{
    "request": {
        "method": "POST",
        "url": "/bouygues/token",
        "basicAuthCredentials": {
            "username": "myClientId",
            "password": "myClientSecret"
        },
        "headers": {
            "Content-Type": {
                "equalTo": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "equalTo": "grant_type=client_credentials"
            }
        ]
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "jsonBody": {
            "access_token": "at-3f2fd75d-2daa-4aa1-b43c-2bf7726e4865",
            "expires_in": 3600,
            "refresh_credit": 0,
            "scope": "EXT_ApplicationName",
            "token_type": "Bearer"
        }
    }
}